{
    "taskGroups": [
        {
            "taskSpec": {
                "runnables": [
                    {
                        "container": {
                            "imageUri": "europe-west1-docker.pkg.dev/protocol-labs-data/pl-data/filet:v0.5.6",
                            "volumes": [
                                "/mnt/share:/gcs",
                                "/mnt/snapshots/historical-exports:/snapshots"
                            ],
                            "entrypoint": "/lily/export.sh",
                            "commands": [
                                "/snapshots/snapshot_2295360_2298242_1667419153.car.zst",
                                "/gcs/backfill/"
                            ],
                            "options": "--privileged -e LILY_BLOCKSTORE_CACHE_SIZE=7000000 -e LILY_STATESTORE_CACHE_SIZE=4500000"
                        }
                    },
                    {
                        "script": {
                            "text": "ls /mnt/share"
                        }
                    }
                ],
                "computeResource": {
                    "cpuMilli": 32000,
                    "memoryMib": 131072,
                    "boot_disk_mib": 500000
                },
                "volumes": [
                    {
                        "gcs": {
                            "remotePath": "pl-data-temp"
                        },
                        "mountPath": "/mnt/share"
                    },
                    {
                        "gcs": {
                            "remotePath": "fil-mainnet-archival-snapshots"
                        },
                        "mountPath": "/mnt/snapshots"
                    }
                ],
                "maxRetryCount": 1,
                "maxRunDuration": "86400s"
            },
            "taskCount": 1,
            "parallelism": 1
        }
    ],
    "allocationPolicy": {
        "instances": [
            {
                "policy": {
                    "machineType": "n2-standard-32",
                    "provisioningModel": "SPOT"
                }
            }
        ]
    },
    "logsPolicy": {
        "destination": "CLOUD_LOGGING"
    }
}
